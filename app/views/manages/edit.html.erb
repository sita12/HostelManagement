<div class = "container">
    <div class = "row">
      <div class = "col-xs-12 col-md-6 col-md-offset-3">
<%= simple_form_for @manage, method: :patch do |f| %>
<%= f.input :name %>
  <%= f.input :room %>
  <%= f.input :location %>
  <%= f.input :phone %>
  <%= f.input :date %>
  <%= f.submit 'Update Record', class: 'btn btn-primary' %>
<% end %>
      </div>
    </div>
</div>       